@InProceedings{ LNCS2439:ID197:pp665,
		  author = 	 { M.G. Arenas and Pierre Collet and A.E. Eiben and M'ark
				Jelasity and J.J. Merelo and Ben Paechter and Mike Preu\ss and Marc Schoenauer},
		    title = 	 {A Framework for Distributed Evolutionary Algorithms},
crossref={ppsn2002},
  note = 	 {Keywords: Implementation::Parallel EAs,
                  Related::Simulated Adaptive Behavior,
                  Technique::Advanced techniques - miscellaneous,
                  Technique::Evolutionary computing - general,
                  Technique::Evolutionary computing - miscellaneous,
                  \url{CiteSeer
                  context}{http://citeseer.nj.nec.com/context/2189070/0}, available from \url{http://link.springer.de/link/service/series/0558/papers/2439/243900665.pdf}{http://link.springer.de/link/service/series/0558/papers/2439/243900665.pdf}, \url{Metapress}{http://www.springerlink.com/link.asp?id=h4n29kbl69jvab4c} },
  abstract = {his paper describes the recently released DREAM

  (Distributed Resource Evolutionary Algorithm Machine) framework for

  the automatic distribution of evolutionary algorithm (EA) processing

  through a virtual machine built from large numbers of individual

  machines linked by  standard Internet protocols. The
  framework allows five different user entry points which depend on
  the knowledge and requirements of the user. At the highest level,
  users may specify and run distributed EAs simply by manipulating
  graphical displays. At the lowest level the framework turns becomes a
  P2P (Peer to Peer)  mobile agent system, that may be used for
  the automatic distribution of a class of processes including, but
  not limited to, EAs.}
}


